<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.web>
        <customErrors>
            <error redirect="http://192.168.1.101/inetpub/wwwroot/404/404.html" statusCode="404" />
        </customErrors>
    </system.web>
    <system.webServer>
        <httpErrors>
            <remove statusCode="404" subStatusCode="-1" />
